<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         <welcome-file-list>
         <welcome-file>index.jsp</welcome-file>
         </welcome-file-list>
         
         <servlet-mapping>
         <servlet-name>Login</servlet-name>
         <url-pattern>/login</url-pattern>
         </servlet-mapping>
         
         <servlet>
         <servlet-name>Login</servlet-name>
         <servlet-class>com.te.loginApp.LoginLogic</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>register</servlet-name>
         <url-pattern>/register</url-pattern>
         </servlet-mapping>
         
         <servlet>
            <servlet-name>register</servlet-name>
             <servlet-class>com.te.loginApp.RegistrationLogic</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>notregister</servlet-name>
         <url-pattern>/notregistered</url-pattern>
         </servlet-mapping>
         
         <servlet>
            <servlet-name>notregister</servlet-name>
             <servlet-class>com.te.loginApp.RegistrationLogic</servlet-class>
         </servlet>
         
         <servlet-mapping>
         <servlet-name>relogin</servlet-name>
         <url-pattern>/relogin</url-pattern>
         </servlet-mapping>
         
         <servlet>
         <servlet-name>relogin</servlet-name>
         <servlet-class>com.te.loginApp.LoginLogic</servlet-class>
         </servlet>
</web-app>